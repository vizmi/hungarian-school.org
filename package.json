{
  "name": "east-bay-hun-school",
  "version": "0.1.0",
  "description": "East Bay Hungarian Educational Group homepage",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "https://vizmi@east-bay-hun-school.scm.azurewebsites.net/east-bay-hun-school.git",
  "scripts": {
    "start": "node server/index.js",
    "debug": "cross-env DEBUG=express:* node server/index.js",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "build": "cross-env NODE_ENV=production webpack src/index.js --config webpack.config.production.js",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test",
    "prebuild": "npm run clean && npm run check",
    "release": "git add . && git commit -m \"production release\" && git push github master",
    "_postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "_test:cov": "cross-env NODE_ENV=test babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha -- --recursive --require ./test/setup.js"
  },
  "devEngines": {
    "node": "4.4.7",
    "npm": "3.10.3"
  },
  "engines": {
    "node": "4.4.7",
    "npm": "3.10.3"
  },
